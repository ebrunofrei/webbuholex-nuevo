  "scripts": {
    "frontend": "vite",
    "backend": "nodemon server.js",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "start": "node server.js",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/features/noticias/**/*.{js,jsx} src/components/ui/ReaderModal.jsx src/services/noticiasClientService.js src/services/noticiasContenido.js src/services/vozService.js",
    "prepare": "husky install",
    "deploy": "git add . && git commit -m 'deploy autom√°tico' && git push origin main",
    "db:test": "node ./scripts/db-test.js",
    "sync-assets": "powershell -Command \"New-Item -ItemType Directory -Force -Path public/assets; Copy-Item -Path backend/public/* -Destination public/assets -Recurse\"",
    "test:news-all": "node scripts/test-news-all.js",
    "clean": "rimraf node_modules/.cache",
    "fix:imports": "node scripts/fix-imports.mjs",
    "fix:alias": "node scripts/fix-alias.mjs",
    "fix:db": "node scripts/fix-db.mjs",
    "fix:all": "node scripts/fix-all.mjs",
    "fix:especialidades": "dotenv -e ./backend/.env -- dotenv -e ./.env.local -- dotenv -e ./.env.development -- node ./backend/scripts/fix-especialidades.js",
    "fix:especialidades:dry": "dotenv -e ./backend/.env -- dotenv -e ./.env.local -- dotenv -e ./.env.development -- node ./backend/scripts/fix-especialidades.js --dry",
    "fix:frontend-alias": "node scripts/fix-frontend-alias.mjs",
    "seed:noticias": "node ./scripts/seed-noticias.js",
    "seed:dummy": "node ./scripts/seed-dummys.js",
    "seed:rss": "dotenv -e ./backend/.env -- dotenv -e ./.env.local -- dotenv -e ./.env.development -- node ./backend/scripts/seed-rss.js",
    "test": "NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js -i",
    "smoke:providers": "node ./backend/scripts/smoke-providers.js",
    "news:once": "node ./backend/jobs/cronNoticias.js",
    "news:save": "node ./backend/jobs/saveNoticiasJob.js",
    "news:cron": "node ./jobs/cronNoticias.js",
    "news:seed": "node ./scripts/seed-noticias.js",
    "test:extract": "node ./backend/scripts/test-extract.js",
    "bf:fuente": "node ./scripts/backfill-fuente-norm.mjs",
    "bf:fuente:dry": "node ./scripts/backfill-fuente-norm.mjs --dry",
    "bf:esp": "node ./scripts/backfill-especialidad.mjs",
    "bf:esp:dry": "node ./scripts/backfill-especialidad.mjs --dry"
  },
